   "source": [
    "import random\n",
    "\n",
    "\n",
    "def threeDoors(N, p):\n",
    "    options = []\n",
    "    opened_doors = []\n",
    "    closed_doors = []\n",
    "    final_doorchoice = 0\n",
    "    e = 1\n",
    "\n",
    "    for b in range(N):\n",
    "        options.append(b)\n",
    "\n",
    "    first_pick = random.choice(options)\n",
    "    \n",
    "    doors = [\"Car\"]\n",
    "    for a in range(0, N - 1):\n",
    "        doors.append(\"Goat\")\n",
    "\n",
    "    random.shuffle(doors)\n",
    "\n",
    "    for t in range(N):\n",
    "        if t != first_pick and doors[t] == \"Goat\" and e <= N - 2:\n",
    "            opened_doors.append(t)\n",
    "            e += 1\n",
    "    \n",
    "        elif t != first_pick:\n",
    "            closed_doors.append(t)\n",
    "    \n",
    "    \n",
    "    switch_options = [\"yes\", \"no\"]\n",
    "    switch_response = random.choice(switch_options)\n",
    "\n",
    "    if switch_response == \"yes\":\n",
    "        final_doorchoice = random.choice(closed_doors)\n",
    "\n",
    "    if switch_response == \"no\":\n",
    "        final_doorchoice = first_pick\n",
    "\n",
    "    print(\"%5d -- %5d -- %7s -- %7s\" % (N, p, switch_response, doors[final_doorchoice]))\n",
    "\n",
    "\n",
    "def main():\n",
    "    Trials = 10000 #enter number of trials you want to run\n",
    "    N = 3 #enter total number of doors you want in the game\n",
    "    p = 1 #enter total number of doors you want the host to open\n",
    "\n",
    "    print(\"%5s    %5s    %7s     %7s\" % (\"N\", \"p\", \"Switch\", \"Prize\"))\n",
    "\n",
    "    for i in range(Trials):\n",
    "        threeDoors(N, p)\n",
    "        N += 1\n",
    "        p += 1\n",
    "\n",
    "\n",
    "main() "
   ]
